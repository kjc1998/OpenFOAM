FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    rotatingAMI1
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    rotatingAMI2
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    inlet
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    room
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    desk
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    fan
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
}

