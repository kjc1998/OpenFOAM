FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    rotatingAMI1
    {
        type            cyclicAMI;
        value           $internalField;
    }
    rotatingAMI2
    {
        type            cyclicAMI;
        value           $internalField;
    }
    inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    1.2;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    housing
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    impeller
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    cover
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}


// ************************************************************************* //
