FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
	location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-5;

boundaryField
{
    rotatingAMI1
    {
        type            cyclicAMI;
        value           uniform 1e-5;
    }
    rotatingAMI2
    {
        type            cyclicAMI;
        value           uniform 1e-5;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    housing
    {
        type            nutkWallFunction;
        value           uniform 1e-5;
    }
    impeller
    {
        type            nutkWallFunction;
        value           uniform 1e-5;
    }
    cover
    {
        type            nutkWallFunction;
        value           uniform 1e-5;
    }
}

