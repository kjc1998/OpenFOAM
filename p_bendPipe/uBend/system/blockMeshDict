FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale   1;

// gap 60
min_x	-1;
max_x	3;

// gap 260
min_y	-10;
max_y	250;

// gap 170
min_z	-10;
max_z	160;


vertices
(
    ($min_x $min_y $min_z)	// 0
    ($max_x $min_y $min_z) 	// 1
    ($max_x $max_y $min_z)	// 2
    ($min_x $max_y $min_z)	// 3
	
    ($min_x $min_y $max_z)	// 4
    ($max_x $min_y $max_z) 	// 5
    ($max_x $max_y $max_z)	// 6
    ($min_x $max_y $max_z)	// 7
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (1 65 45) simpleGrading (1 1 1)
);

edges
(
);

// clockwise from inside the block
boundary
(
);

